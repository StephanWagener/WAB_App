<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerColor="#ff888888"
                android:centerY="0.5"
                android:endColor="#ffaaaaaa"
                android:startColor="#ffcccccc" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/study_test_color"
                    android:centerY="0.5"
                    android:endColor="#00aa00"
                    android:startColor="#00cc00" />
            </shape>
        </clip>
    </item>
</layer-list>